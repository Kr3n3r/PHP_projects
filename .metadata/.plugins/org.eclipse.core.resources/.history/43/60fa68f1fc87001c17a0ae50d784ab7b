<?php 
include_once '../utils.php';
catchRequired("rental_id");

$query = "update rental set return_date=now() where rental_id=$rental_id";


if ($stmt = $con->prepare($query)) {
    $stmt->execute();
    echo "Inserción realizada.";
}
else{
    echo "Inserción no realizada.";
}
$stmt->close();
$con->close();
?>
